{
  "name": "mr-mateus-zup-movies",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "clean": "rimraf ./dist",
    "start": "node server.js",
    "start:host": "ng serve --host 0.0.0.0",
    "build:lib": "ng build --project=zup-movies",
    "build": "ng build",
    "build:prod": "ng build --prod",
    "test": "ng test",
    "test:ci": "ng test --watch=false --browsers=ChromeHeadlessNoSandbox --code-coverage --karma-config=karma.conf.js",
    "test:single-run": "ng test -- --watch=false --code-coverage",
    "e2e": "ng e2e",
    "lint": "ng lint",
    "build:ci": "npm run clean && ng lint && npm run build:prod && npm run test:ci && ng e2e",
    "postinstall": "ng build --prod",
    "heroku-postbuild": "ng build --prod"
  },
  "engines": {
    "node": "10.16.3",
    "npm": "6.9.0"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.0.0",
    "@angular/common": "~8.0.0",
    "@angular/compiler": "~8.0.0",
    "@angular/core": "~11.0.5",
    "@angular/forms": "~8.0.0",
    "@angular/platform-browser": "~8.0.0",
    "@angular/platform-browser-dynamic": "~8.0.0",
    "@angular/router": "~8.0.0",
    "bootstrap": "^3.4.1",
    "express": "^4.17.1",
    "karma-spec-reporter": "0.0.32",
    "ngx-device-detector": "^1.4.1",
    "ngx-loading": "^8.0.0",
    "path": "^0.12.7",
    "rxjs": "~6.4.0",
    "tslib": "^1.9.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.803.20",
    "@angular-devkit/build-ng-packagr": "^0.803.20",
    "@angular/cli": "~8.0.0",
    "@angular/compiler-cli": "~8.0.0",
    "@angular/language-service": "~8.0.0",
    "@types/jasmine": "2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "caniuse-lite": "^1.0.30000997",
    "codelyzer": "^5.0.0",
    "enhanced-resolve": "^3.3.0",
    "jasmine-core": "~2.8.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.1.3",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~1.4.2",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "~0.2.2",
    "karma-spec-reporter": "0.0.32",
    "ng-packagr": "^5.1.0",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tsickle": "^0.35.0",
    "tslint": "~5.15.0",
    "typescript": "~3.4.3"
  }
